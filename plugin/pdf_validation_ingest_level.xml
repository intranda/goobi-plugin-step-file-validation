<config>
		<profile name="testprofile" >
			<level>
				<check name="checkVersion"
					tool="jhove" 
					code="errorCode" 
					xpath="count(//jhove:repInfo/jhove:format)"
					regEx="^1\.[012456]$|^2\.0$" 
				/>
			</level>
			<level>
				<check name="jhove validation"
					tool="jhove" 
					code="validation_failed" 
					xpath="//jhove:repInfo/jhove:status"
					regEx="Well-Formed and valid" 
				/>
			</level>
			<level>
				<check name="pdf-a validation"
					tool="verapdf" 
					code="pdfa_validation_failed" 
					xpath="xpathSelector"
					regEx="regEx" 
				/>
			</level>
		</profile>
		<tools>
			<veraPdf configFolder="" />
			<jhove configFolder="" />
			<dorid configFolder="" />
		</tools>
	</config>